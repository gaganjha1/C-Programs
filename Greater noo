#include <stdio.h>

int main() {
    // Declare and initialize an array
    int arr[5] = {5, 10, 15, 20, 25};
    int x = 13;

    // Variable to count numbers greater than x
    int great = 0;

    // Loop through the array to find numbers greater than x
    for (int i = 0; i < 5; i++) {
        if (x < arr[i]) {
            great++;
        }
    }

    // Print the result
    printf("%d\n", great);

    return 0;
}
